<div class="formContainer">  <form class="form-horizontal userForm" role="form" action="/process?form=userForm" method="POST">      <input type="hidden" name="_csrf" value="{{ csrf }}">      <input type="hidden" name="userID" value="{{authId}}">      <input type="hidden" name="userType" value="{{userType}}" id="buttonValue"/>      <div class="form-group">        <label for="fieldName" class="col-sm-2 control-label">Name</label>        <div class="col-sm-4">          <input type="text" class="form-control" id="fieldName" name="name" value="{{displayName}}">        </div>      </div>      <div class="form-group">          <label for="fieldEmail" class="col-sm-2 control-label">Email</label>          <div class="col-sm-4">            <input type="email" class="form-control" required id="fieldName" name="email" value="{{mail}}">          </div>      </div>      <div class="form-group">          <label for="fieldEmail" class="col-sm-2 control-label">Specialty</label>          <div class="col-sm-4">          <div class="btn-group" data-toggle="buttons-radio">                <button type="button" class="btn btn-primary hacker">Hacker</button>                <button type="button" class="btn btn-primary hustler">Hustler</button>                <button type="button" class="btn btn-primary designer">Designer</button>            </div>        </div>      </div>      <div class="form-group">          <label for="fieldEmail" class="col-sm-2 control-label">Skills</label>          <div class="col-sm-4">            <input type="type" class="form-control" id="input-tags" name="skills" value="{{skills}}">          </div>      </div>      <div class="container">      <div class="row">      <div class="col-sm-4">          <div class="thumbnail">            <img src="http://p1.pichost.me/i/44/1674639.jpg" alt="pic 1">            <div class="caption">              <h3>Thumbnail label</h3>              <p>picture number 1</p>              <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>            </div>          </div>        </div>        <div class="col-sm-4">          <div class="thumbnail">            <img src="http://www.indianewengland.com/Media/PublicationsArticle/hamed-resized.jpg" alt="pic 1">            <div class="caption">              <h3>Thumbnail label 2</h3>              <p>picture number 1</p>              <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>            </div>          </div>        </div>        <div class="col-sm-4">          <div class="thumbnail">            <img src="http://blogimg.ngfiles.com/752000/752941/107016531_xl-1.jpg" alt="pic 1>            <div class="caption">              <h3>Thumbnail label 3</h3>              <p>picture number 1</p>              <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>            </div>          </div>        </div>    </div></div>      <div class="form-group">        <div class="col-sm-offset-2 col-sm-4">          <button type="submit" class="btn btn-default">Register</button>        </div>      </div>   </form></div>{{#section 'jquery'}}  <script>  $(document).ready(function(){      var skillNames = ["Node.js","Java","Rails"];      var allSkills = [];      for (i=0; i < skillNames.length; i++) {          allSkills.push({name:skillNames[i]});      }      $('#input-tags').selectize({            delimiter: ',',            persist: false,            valueField: 'name',            labelField: 'name',            searchField: 'name',            create: function(input) {                return {                    name: input                }            },            options: allSkills,            openOnFocus:false,            closeAfterSelect:true        });      if ($("#buttonValue").val() == "Hustler") {          $(".hustler").addClass("active");      }      if ($("#buttonValue").val() == "Hacker") {          $(".hacker").addClass("active");      }      if ($("#buttonValue").val() == "Designer") {          $(".designer").addClass("active");      }      $(".btn-group button").click(function () {          $(this).addClass("current").siblings().removeClass("active");          $("#buttonValue").val($(this).text());     });    $('.userForm').on('submit', function(evt){        evt.preventDefault();        var action = $(this).attr('action');        var $container = $(this).closest('.Message');        $.ajax({          url: action,          type: 'POST',          data: $('.userForm').serialize(),          success: function(data){            if(data.success){              $container.html('<h2>Thank you!</h2>');            }            else {              $container.html('There was a problem.');            }          },          error: function(){          $container.html('There was a problem.');          }});    });  });  </script>{{/section}}